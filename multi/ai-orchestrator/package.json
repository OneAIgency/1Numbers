{
  "name": "ai-orchestrator",
  "version": "2.0.0",
  "private": true,
  "description": "Enterprise AI Development Orchestration Platform",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "dev": "turbo run dev",
    "build": "turbo run build",
    "test": "turbo run test",
    "lint": "turbo run lint",
    "clean": "turbo run clean && rm -rf node_modules",
    "api:dev": "cd packages/api && uvicorn src.api.main:app --reload --port 8000",
    "cli:dev": "cd packages/cli && npm run dev",
    "web:dev": "cd packages/web && npm run dev",
    "db:migrate": "cd packages/api && alembic upgrade head",
    "db:reset": "cd packages/api && alembic downgrade base && alembic upgrade head"
  },
  "devDependencies": {
    "turbo": "^2.3.0",
    "typescript": "^5.8.0",
    "vitest": "^4.0.17"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "packageManager": "npm@10.0.0",
  "dependencies": {
    "vite": "^6.4.1"
  }
}
